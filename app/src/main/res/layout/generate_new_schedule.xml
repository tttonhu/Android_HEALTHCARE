<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="214dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout33"
                android:layout_width="411dp"
                android:layout_height="65dp"
                android:background="@color/lv2"
                android:paddingVertical="@dimen/distance_lv1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageButton
                    android:id="@+id/btn_back_generate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:background="@color/lv2"
                    android:src="@drawable/left"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/itim"
                    android:text="Water tracker"
                    android:textColor="@color/lv5"
                    android:textSize="@dimen/title_size"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout34"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout33">

                <TextView
                    android:id="@+id/textView70"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:layout_marginVertical="@dimen/distance_lv1"
                    android:fontFamily="@font/cabin"
                    android:text="Daily goal drink water(ml)"
                    android:textColor="@color/lv1"
                    android:textSize="@dimen/text_size"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <EditText
                    android:id="@+id/ed_goalDrink_generate"
                    android:layout_width="@dimen/distance_lv12"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:layout_marginVertical="@dimen/distance_lv1"
                    android:background="@drawable/boder"
                    android:backgroundTint="@color/white"
                    android:elevation="@dimen/distance_small1"
                    android:fontFamily="@font/cabin"
                    android:text="0"
                    android:textAlignment="center"
                    android:textColor="@color/lv1"
                    android:textSize="@dimen/text_size"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView72"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:layout_marginVertical="@dimen/distance_lv5"
                    android:fontFamily="@font/cabin"
                    android:text="Size(ml)"
                    android:textColor="@color/lv1"
                    android:textSize="@dimen/text_size"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView70" />

                <EditText
                    android:id="@+id/ed_size_generate"
                    android:layout_width="@dimen/distance_lv12"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:layout_marginVertical="@dimen/distance_lv5"
                    android:background="@drawable/boder"
                    android:backgroundTint="@color/white"
                    android:elevation="@dimen/distance_small1"
                    android:fontFamily="@font/cabin"
                    android:inputType="number"
                    android:text="0"
                    android:textAlignment="center"
                    android:textColor="@color/lv1"
                    android:textSize="@dimen/text_size"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ed_goalDrink_generate" />

                <TextView
                    android:id="@+id/textView73"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:layout_marginVertical="@dimen/distance_lv7"
                    android:fontFamily="@font/cabin"
                    android:text="Time sleep"
                    android:textColor="@color/lv1"
                    android:textSize="@dimen/text_size"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView72" />

                <Button
                    android:id="@+id/btn_timeSleep_end_generate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:layout_marginVertical="@dimen/distance_lv5"
                    android:backgroundTint="@color/white"
                    android:elevation="@dimen/distance_small1"
                    android:fontFamily="@font/cabin"
                    android:inputType="number"
                    android:text="22:00"
                    android:textAlignment="center"
                    android:textColor="@color/lv1"
                    android:textSize="@dimen/text_size"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ed_size_generate" />

                <Button
                    android:id="@+id/btn_timeSleep_start_generate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:layout_marginVertical="@dimen/distance_lv5"
                    android:backgroundTint="@color/white"
                    android:elevation="@dimen/distance_small1"
                    android:fontFamily="@font/cabin"
                    android:inputType="number"
                    android:text="06:00"
                    android:textAlignment="center"
                    android:textColor="@color/lv1"
                    android:textSize="@dimen/text_size"
                    app:layout_constraintEnd_toStartOf="@+id/btn_timeSleep_end_generate"
                    app:layout_constraintTop_toBottomOf="@+id/ed_size_generate" />


                <EditText
                    android:id="@+id/ed__generate"
                    android:layout_width="@dimen/distance_lv12"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:layout_marginVertical="@dimen/distance_lv1"
                    android:background="@drawable/boder"
                    android:backgroundTint="@color/white"
                    android:elevation="@dimen/distance_small1"
                    android:fontFamily="@font/cabin"
                    android:inputType="number"
                    android:text="0"
                    android:textAlignment="center"
                    android:textColor="@color/lv1"
                    android:textSize="@dimen/text_size"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView74"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:layout_marginVertical="@dimen/distance_lv5"
                    android:fontFamily="@font/cabin"
                    android:text="Interval reminder(min)"
                    android:textColor="@color/lv1"
                    android:textSize="@dimen/text_size"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn_timeSleep_start_generate" />

                <EditText
                    android:id="@+id/ed_intervalReminder_generate"
                    android:layout_width="@dimen/distance_lv12"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:layout_marginVertical="@dimen/distance_lv5"
                    android:background="@drawable/boder"
                    android:backgroundTint="@color/white"
                    android:elevation="@dimen/distance_small1"
                    android:fontFamily="@font/cabin"
                    android:inputType="number"
                    android:text="0"
                    android:textAlignment="center"
                    android:textColor="@color/lv1"
                    android:textSize="@dimen/text_size"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn_timeSleep_start_generate" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout40"
                android:layout_width="match_parent"
                android:layout_height="281dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout34">

                <GridView
                    android:id="@+id/gv_waterTracker_generate"
                    android:layout_width="368dp"
                    android:layout_height="281dp"
                    android:layout_marginHorizontal="@dimen/distance_lv2"
                    android:layout_marginVertical="@dimen/distance_lv2"
                    android:horizontalSpacing="@dimen/distance_small1"
                    android:numColumns="4"
                    android:verticalSpacing="@dimen/distance_lv1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:listitem="@layout/list_water_tracker" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <Button
                android:id="@+id/btn_newShedule_generate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/cabin"
                android:text="Generate new schedule"
                android:textSize="@dimen/text_size"
                android:backgroundTint="@color/lv2"
                android:textAlignment="center"
                android:layout_marginVertical="@dimen/distance_lv2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout40" />

            <Button
                android:id="@+id/btn_save_genrate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Save"
                android:backgroundTint="@color/lv2"
                android:textSize="@dimen/text_size"
                android:fontFamily="@font/cabin"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="@+id/btn_newShedule_generate"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout40" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>